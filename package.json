{
  "name": "dismusic",
  "version": "2.1.3",
  "description": "An easy music package for discord.js version 14",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run build && node __tests__/test.js",
    "docs": "jsdoc -c docs_config/jsdoc.json",
    "json:docs": "docgen src --source ./src --custom docs_config/docs.yml --output docs_config/docs.json --spaces 2",
    "build": "npm run lint && tsc && npm run format",
    "format": "prettier --write \"lib/**/*.ts\" \"dist/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "author": "retro_ig",
  "license": "MIT",
  "dependencies": {
    "@discordjs/voice": "^0.14.0",
    "@types/node": "^18.14.6",
    "axios": "^1.3.4",
    "clean-jsdoc-theme": "^4.1.8",
    "discord.js": "^14.7.1",
    "ffmpeg-static": "^5.0.2",
    "isomorphic-unfetch": "^3.1.0",
    "libsodium-wrappers": "^0.7.10",
    "play-dl": "^1.9.6",
    "request": "^2.88.2",
    "spotify-url-info": "^3.2.3",
    "tiny-typed-emitter": "^2.1.0",
    "typedoc-nextra": "^0.1.2",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@devsnowflake/docgen": "https://github.com/CesiumLabs/docgen.git",
    "prettier": "^2.8.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/retrouser955/dismusic"
  },
  "homepage": "https://retrouser955.github.io/dismusic/",
  "keywords": [
    "discord",
    "discord.js",
    "discord-music",
    "music",
    "youtube",
    "spotify",
    "soundcloud",
    "framework"
  ]
}
