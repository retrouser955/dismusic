{
  "name": "dismusic",
  "version": "2.1.3",
  "description": "An easy music package for discord.js version 14",
  "main": "dist/index.js",
  "scripts": {
    "test:build": "npm run build && node __tests__/test.mjs",
    "docs": "jsdoc -c docs_config/jsdoc.json",
    "json:docs": "docgen src --source ./src --custom docs_config/docs.yml --output docs_config/docs.json --spaces 2",
    "build": "npm run lint && tsc && npm run format",
    "format": "prettier --write \"lib/**/*.ts\" \"dist/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "node __tests__/test.mjs"
  },
  "author": "retro_ig",
  "license": "MIT",
  "dependencies": {
    "@discordjs/voice": "^0.14.0",
    "@mithron/deezer-music-metadata": "^1.0.3",
    "axios": "^1.3.4",
    "chalk": "^5.2.0",
    "discord.js": "^14.7.1",
    "libsodium-wrappers": "^0.7.10",
    "lyrics-finder": "^21.7.0",
    "play-dl": "^1.9.6",
    "prism-media": "^1.3.5",
    "tiny-typed-emitter": "^2.1.0",
    "yt-stream": "^1.4.5",
    "ytdl-core": "^4.11.4"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "prettier": "^2.8.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.23.26",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/retrouser955/dismusic"
  },
  "homepage": "https://retrouser955.github.io/dismusic/",
  "keywords": [
    "discord",
    "discord.js",
    "discord-music",
    "music",
    "youtube",
    "spotify",
    "soundcloud",
    "framework"
  ],
  "peerDependencies": {
    "@discordjs/opus": "^0.9.0",
    "ffmpeg-static": "^5.0.2",
    "yt-stream": "^1.4.5"
  }
}
